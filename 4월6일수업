#include <stdio.h>

int main()
{
	int a = 0;
	int b = 1;
	scanf("%d", &a);

	for (; a != 1; a--)
	{
		b *= a;
	}
	printf("%d", b);
	return 0;

	for (int a = 0; a < 5; a++)
	{
		for (int b = 0; b < 5; b++)
		{
			printf("*");
		}
		printf("\n");
	}
	return 0;

	for (int a = 1; a < 6; a++)
	{
		for (int b = a; b > 0; b--)
		{
			printf("*");
		}
		printf("\n");
	}
	return 0;

	int d = 4;
	for (int a = 1; a < 6; a++)
	{
		for (int b = 0; b < a - 1; b++)
		{
			printf(" ");
		}
		for (int b = 0; b < d * 2 + 1; b++)
		{
			printf("*");
		}
		d -= 1;
		printf("\n");
	}

	int d = 4;
	for (int a = 1; a < 6; a++)
	{
		for (int b = 0; b < a - 1; b++)
		{
			printf(" ");
		}
		for (int b = 0; b < d * 2 + 1; b++)
		{
			printf("*");
		}
		d -= 1;
		printf("\n");
	}
	for (int a = 1; a < 5; a++)
	{
		for (int b = a - 6; b < -2; b++)
		{
			printf(" ");
		}
		for (int b = 0; b < a * 2 + 1; b++)
		{
			printf("*");
		}
		printf("\n");
	}
	return 0;

	int i, j, k;
	for (i = 1; i <= 5; i++)
	{
		for (k = 5 - i; k > 0; k--)
		{
			printf(" ");
		}
		for (j = 0; j < i * 2 - 1; j++)
		{
			printf("*");
		}
			printf("\n");
		}
	for (i = 4; i >= 0; i--)
	{
		for (k = 0; k < 5 - i; k++)
		{
			printf(" ");
		}
		for (j = i * 2 - 1; j > 0; j--)
		{
			printf("*");
		}
		printf("\n");
	}
	return 0;
}
