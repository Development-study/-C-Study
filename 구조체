#include<stdio.h>

//구조체 structure

struct Point {//구조체로 점을 정의
	int x, y;
};

int main() {
	struct Point p = { 4,5 };
	printf("x = %d y = %d\n", p.x, p.y);
	return 0;
}

// Point|---------------------| Point 구조체
//	|     x         y     |
//	|  |-----|   |-----|  |
//	|  |     |   |     |  |
//	|  |-----|   |-----|  |
//	|---------------------|

//     P|---------------------|
//	|     x         y     |
//	|  |-----|   |-----|  |
//	|  |  4  |   |  5  |  |
//	|  |-----|   |-----|  |
//	|---------------------|
//	p.x = 4 // p.y = 5
