#include<stdio.h>

int main() {
	int a[20] = { 35,26,48,77,56,98,75,78,99,0,43,56,67,84,99,100,45,37,49,78 };
	int i, j, histo[11] = { 0 };
	for (i = 0; i < 20; i++)
		histo[a[i] / 10]++;
	for (i = 0; i <= 10; i++) {
		printf("%3d : ", i * 10);
		for (j = 0; j < histo[i]; j++) {
			printf("ï¹›");
		}
		printf("\n");
	}

	return 0;
}
